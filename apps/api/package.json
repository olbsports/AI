{
  "name": "@horse-vision/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build && tsc-alias",
    "build:deploy": "prisma generate && prisma db push --accept-data-loss && nest build && tsc-alias",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "prisma": "^5.8.1",
    "typescript": "^5.3.3",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^6.4.14",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@horse-vision/config": "workspace:*",
    "@horse-vision/core": "workspace:*",
    "@horse-vision/types": "workspace:*",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^5.1.1",
    "@prisma/client": "^5.8.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0",
    "stripe": "^14.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.3.0",
    "@types/passport-jwt": "^4.0.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "vitest": "^1.2.1"
  }
}
